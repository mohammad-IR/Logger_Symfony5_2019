{% set i = 1 %}
{% for item in bought_packages %}
    <tr>
        <th scope="row">{{ i }}</th>
        <td>{{ item.namePackage.name }}</td>
        <td>{{ item.namePackage.price }}</td>
        <td>{{ item.namePackage.numberApp }}</td>
        <td>{{ item.namePackage.limitReport }}</td>
    </tr>
    {% set i = i + 1 %}
{% endfor %}